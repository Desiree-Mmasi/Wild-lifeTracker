{{#partial "content"}}
    <div class="view  animal overlay">
        <img class="card-img-top" src="/images/user.png" alt="Card image cap">
        <a>
            <div class="mask waves-effect waves-light rgba-white-slight"></div>
        </a>
    </div>


    <div class="card-body">


        <a class="activator mr-4"><i class="fas fa-share-alt"></i></a>

        <h4 class="card-title"><strong>New Species</strong></h4>
        <hr>

        <p class="card-text"> Name: <strong>{{species.mSpecies}}</strong></p>
        <p class="card-text"> Type: <strong>{{species.mState}}</strong></p>
        <p class="card-text"> Condition: <strong>{{species.mCondition}}</strong></p>
        <a class="link-text">

        </a>

    </div>
    </div>

{{/partial}}
{{>layout.hbs}}