{{#partial "content"}}
    <div class="sight">
        <div class="view insight overlay">
            <img class="card-img-top" src="/images/trees.jpg" alt="Card image cap">
            <a>
                <div class="mask waves-effect waves-light rgba-white-slight"></div>
            </a>
        </div>


        <div class="card-body sight2">


            <a class="activator mr-4"><i class="fas fa-share-alt"></i></a>

            <h4 class="card-title"><strong>New Sighting</strong></h4>
            <hr>

            <p class="card-text"> Ranger Id: <strong>{{sighting.rangerId}}</strong></p>
            <p class="card-text"> Animal Id: <strong>{{sighting.animalId}}</strong></p>
            <p class="card-text"> Location: <strong>{{sighting.location}}</strong></p>
            <a class="link-text">

            </a>

        </div>
    </div>

{{/partial}}
{{>layout.hbs}}