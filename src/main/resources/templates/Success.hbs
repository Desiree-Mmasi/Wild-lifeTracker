{{#partial "content"}}
    <div class="page">
        <div class="card small ovf-hidden">


            <div class="view overlay">
                <img class="card-img-top" src="/images/user.png" alt="Card image cap">
                <a>
                    <div class="mask waves-effect waves-light rgba-white-slight"></div>
                </a>
            </div>



            <div class="card-body">


                <a class="activator mr-4"><i class="fas fa-share-alt"></i></a>

                <h4 class="card-title"><strong>New Ranger</strong></h4>
                <hr>

                <p class="card-text"> Ranger: <strong>{{ranger.rangerName}}</strong></p>
                <p class="card-text"> Gender: <strong>{{ranger.rangerGender}}</strong></p>
                <p class="card-text"> Badge Number: <strong>{{ranger.rangerBadge}}</strong></p>
                <p class="card-text"> Contact: <strong>{{ranger.rangerContact}}</strong></p>
                <p class="card-text"> Site: <strong>{{ranger.rangerSighting}}</strong></p>
                <a class="link-text">

                </a>

            </div>

        </div>
    </div>
{{/partial}}
{{>layout.hbs}}