{{#partial "content"}}
    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <div class="row">
                    <div class="col-md-6"></div>
                    <div class="col-md-6"></div>
                </div>

            <div class="col-md-4"></div>

    <form action='/done' method="post">
    <section class="form-dark">


        <div class="card card-image"
             style="background-image: url('https://www.google.com/url?sa=i&source=images&cd=&ved=2ahUKEwjbmYTH2JjjAhVMJBoKHZoNCScQjRx6BAgBEAU&url=http%3A%2F%2Fwww.renegadetribune.com%2Fnature-the-beginning%2F&psig=AOvVaw0AHbe0aeuxAI8TTOdwJfSv&ust=1562241286134280'); width: 100%; height: 100%;">
            <div class="text-white rgba-stylish-strong py-5 px-5 z-depth-4">


                <div class="text-center">
                    <h3 class="white-text mb-5 mt-4 font-weight-bold"><strong>SIGN</strong> <a
                            class="green-text font-weight-bold"><strong> UP</strong></a></h3>
                </div>


                <div class="md-form">
                    <input type="text" name="name" id="name" class="form-control white-text">
                    <label for="Form-ranger5">Ranger Name:</label>
                </div>

                <div class="md-form">
                    <input type="text" name="number" id="number" class="form-control white-text">
                    <label for="Form-ranger5">Badge Number:</label>
                </div>

                <div class="md-form">
                    <input type="text" name="contact" id="contact" class="form-control white-text">
                    <label for="Form-ranger5">Contact:</label>
                </div>

                <div class="md-form">
                    <input type="text" name="site" id="site" class="form-control white-text">
                    <label for="Form-ranger5">Site Allocated:</label>
                </div>

                <div class="row d-flex align-items-center mb-4">
                    <div class="text-center mb-3 col-md-12">
                        <button type="button" class="btn btn-success btn-block btn-rounded z-depth-1">Sign in</button>
                    </div>

                </div>

                <div class="col-md-12">
                    <p class="font-small white-text d-flex justify-content-end">Have an account? <a href="#"
                                                                                                    class="green-text ml-1 font-weight-bold"> Log in</a></p>
                </div>

            </div>
        </div>


    </section>
    </form>
            </div>
        </div>
    </div>


        #if($Ranger)
        <div class="card ovf-hidden">


            <div class="view overlay">
                <img class="card-img-top" src="https://mdbootstrap.com/img/Photos/Others/photo11.jpg" alt="Card image cap">
                <a>
                    <div class="mask waves-effect waves-light rgba-white-slight"></div>
                </a>
            </div>


            <div class="card-body">


                <a class="activator mr-4"><i class="fas fa-share-alt"></i></a>

                <h4 class="card-title">New Ranger:</h4>
                <hr>

                <p class="card-text"> Ranger: <strong>$Ranger.getName</strong></p>
                <p class="card-text"> Badge Number: <strong>$Number.getNumber</strong></p>
                <p class="card-text"> Contact: <strong>$Contact.getContact</strong></p>
                <p class="card-text"> Site: <strong>$Site.getSite</strong></p>
                <a class="link-text">

                </a>

            </div>
        </div>


        <div class="card-reveal white">


            <div class="content text-center">

                <h4 class="card-title font-weight-bold my-4">Social shares <i class="fas fa-times text-muted"></i></h4>
                <hr>

                <ul class="list-unstyled list-inline d-flex justify-content-center flex-wrap">
                    <li class="list-inline-item"><a class="btn-floating btn-dribbble"><i class="fab fa-dribbble"> </i></a></li>
                    <li class="list-inline-item"><a class="btn-floating btn-slack"><i class="fab fa-slack-hash"> </i></a></li>
                    <li class="list-inline-item"><a class="btn-floating btn-ins"><i class="fab fa-instagram"> </i></a></li>
                    <li class="list-inline-item"><a class="btn-floating btn-pin"><i class="fab fa-pinterest"> </i></a></li>
                    <li class="list-inline-item"><a class="btn-floating btn-tw"><i class="fab fa-twitter"> </i></a></li>
                    <li class="list-inline-item"><a class="btn-floating btn-gplus"><i class="fab fa-google-plus-g"> </i></a>
                    </li>
                    <li class="list-inline-item"><a class="btn-floating btn-git"><i class="fab fa-github"> </i></a></li>
                </ul>
                <h5 class="font-weight-bold my-4">Join our community</h5>
                <hr>

                <ul class="list-unstyled list-inline d-flex justify-content-center flex-wrap mt-4">
                    <li class="list-inline-item"><a class="btn btn-fb px-4"><i class="fab fa-facebook-f"> </i></a></li>
                    <li class="list-inline-item"><a class="btn btn-tw px-4"><i class="fab fa-twitter"> </i></a></li>
                    <li class="list-inline-item"><a class="btn btn-li px-4"><i class="fab fa-linkedin-in"> </i></a></li>
                    <li class="list-inline-item"><a class="btn btn-ins px-4"><i class="fab fa-instagram"> </i></a></li>
                </ul>

            </div>

        </div>
 {{/partial}}
